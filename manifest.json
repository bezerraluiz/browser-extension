{
  "manifest_version": 3,
  "name": "Key Logger Console",
  "version": "1.0",
  "description": "Escuta e printa todas as teclas pressionadas no console.",
  "permissions": ["scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
